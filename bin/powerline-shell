#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'powerline-shell==0.2.2','console_scripts','powerline-shell'
__requires__ = 'powerline-shell==0.2.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('powerline-shell==0.2.2', 'console_scripts', 'powerline-shell')()
    )
